<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import wifen.client.ui.controllers.ChatController?>

<fx:root maxHeight="-Infinity" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <HBox fx:id="HboxID" >
         <children>
            <ChoiceBox fx:id="choiceID" />
            <Button fx:id="optionID" mnemonicParsing="false" text="Optionen" />
            <Button fx:id="refreshID" mnemonicParsing="false" text="Refresh" />
            <Button fx:id="dcID" mnemonicParsing="false" text="Trennen" />
         </children>
      </HBox>
    </top>
    <right>
      <VBox fx:id="ereignisVBoxID">
         <children>
            <ChatController fx:id="chatBox" />
         </children>
      </VBox>
   </right>
</fx:root>
