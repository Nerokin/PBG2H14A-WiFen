<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<fx:root minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label alignment="CENTER" prefWidth="300.0" text="WiFeN" textAlignment="CENTER" BorderPane.alignment="CENTER">
         <font>
            <Font size="96.0" />
         </font>
      </Label>
   </top>
   <center>
      <VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Sound Optionen" underline="true">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label prefHeight="50.0" text="Volumen:">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Slider id="sliderVolumen" fx:id="sd_volumen" prefHeight="14.0" prefWidth="264.0" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label text="Musik stumm">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <CheckBox id="cbMuteMusik" fx:id="cbx_muteMusic" mnemonicParsing="false" prefHeight="17.0" prefWidth="7.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label text="Sound stumm">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <CheckBox id="cbMuteSound" fx:id="cbx_muteSound" mnemonicParsing="false" prefHeight="17.0" prefWidth="7.0" />
               </children>
            </HBox>
            <Label text="Medien Optionen" underline="true">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Label text="Maximale Dateigröße:">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Slider id="sliderMaxDateigroesse" fx:id="sd_filesize" prefHeight="14.0" prefWidth="264.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button id="btnSpeichern" fx:id="btn_speichern" mnemonicParsing="false" text="Speichern">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button id="btnSpeichern" fx:id="btn_abbrechen" mnemonicParsing="false" text="Abbrechen">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="IP:">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Text>
            <Label fx:id="lb_ip" text="192.168.2.1">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Version:">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <Label fx:id="lb_version" text="0.0.1">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </bottom>
</fx:root>