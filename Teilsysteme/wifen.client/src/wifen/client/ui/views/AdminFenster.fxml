<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<fx:root type="TitledPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<content>
	<VBox>
   <children>
      <HBox prefHeight="40.0" prefWidth="200.0">
         <children>
            <Label text="Rollen">
               <font>
                  <Font size="16.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <Button fx:id="btn_sitzungSpeichern" mnemonicParsing="false" text="Sitzung Speichern">
               <HBox.margin>
                  <Insets left="105.0" />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </HBox>
      <ListView fx:id="lv_rollen" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ListView>
      <Label text="Spieler">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ListView fx:id="lv_spieler" prefHeight="200.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ListView>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="btn_editieren" mnemonicParsing="false" text="Editieren">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_neu" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Neu">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btn_zuweisen" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" text="Zuweisen">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" top="10.0" />
         </VBox.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
  </VBox>
  </content>
</fx:root>
